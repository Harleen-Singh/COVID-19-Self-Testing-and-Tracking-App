<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homePage1">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="136dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo_bg" />

        <EditText
            android:id="@+id/etName"
            android:layout_width="304dp"
            android:layout_height="45dp"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="Enter your name"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.485"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <EditText
            android:id="@+id/etAge"
            android:layout_width="308dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="31dp"
            android:ems="10"
            android:hint="Enter your age(dd/mm/yyyy)"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.242"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etName" />

        <ImageView
            android:id="@+id/ivCld"
            android:layout_width="34dp"
            android:layout_height="35dp"
            android:layout_marginStart="325dp"
            android:layout_marginTop="-40dp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="@+id/etAge"
            app:layout_constraintHorizontal_bias="0.985"
            app:layout_constraintStart_toStartOf="@+id/etAge"
            app:layout_constraintTop_toTopOf="@+id/etAge"
            app:srcCompat="@drawable/cld" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:text="Gender"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.071"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etAge" />

        <Spinner
            android:id="@+id/spGender"
            android:layout_width="305dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.292"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <EditText
            android:id="@+id/etAddress"
            android:layout_width="298dp"
            android:layout_height="42dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:hint="Enter address or auto detect"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.274"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spGender" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="50dp"
            android:layout_height="34dp"
            android:layout_marginStart="320dp"
            android:layout_marginTop="-35dp"
            app:layout_constraintEnd_toEndOf="@+id/etAddress"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/etAddress"
            app:layout_constraintTop_toTopOf="@+id/etAddress"
            app:srcCompat="@drawable/places" />

        <EditText
            android:id="@+id/etSubsidaryPhones"
            android:layout_width="280dp"
            android:layout_height="45dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Enter your other phone numbers one by one"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.318"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etAddress" />

        <ImageView
            android:id="@+id/bAddPhn"
            android:layout_width="89dp"
            android:layout_height="36dp"
            android:layout_marginStart="300dp"
            android:layout_marginTop="-35dp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="@+id/etSubsidaryPhones"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/etSubsidaryPhones"
            app:layout_constraintTop_toTopOf="@+id/etSubsidaryPhones"
            app:srcCompat="@drawable/r10" />

        <ImageView
            android:id="@+id/ivAdd"
            android:layout_width="61dp"
            android:layout_height="18dp"
            android:layout_marginStart="306dp"
            android:layout_marginTop="-32dp"
            android:layout_marginEnd="80dp"
            app:layout_constraintEnd_toEndOf="@+id/bAddPhn"
            app:layout_constraintHorizontal_bias="0.479"
            app:layout_constraintStart_toStartOf="@+id/bAddPhn"
            app:layout_constraintTop_toTopOf="@+id/bAddPhn"
            app:srcCompat="@drawable/a" />

        <ListView
            android:id="@+id/lvSubphones"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/tb"
            android:layout_width="110dp"
            android:layout_height="24dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:text="Travel History"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.071"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/etSource"
            android:layout_width="326dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:ems="10"
            android:hint="Enter source "
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.207"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tb" />

        <EditText
            android:id="@+id/etDestination"
            android:layout_width="322dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:ems="10"
            android:hint="Enter Destination"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.262"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etSource" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="340dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:text="Exposure with COVID affected person"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.095"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etDestination" />

        <Switch
            android:id="@+id/swExposure"
            android:layout_width="203dp"
            android:layout_height="27dp"
            android:layout_marginTop="16dp"
            android:text="Exposure"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.099"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <Switch
            android:id="@+id/swFace"
            android:layout_width="201dp"
            android:layout_height="31dp"
            android:layout_marginTop="8dp"
            android:text="Face to Face Exposure"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.121"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swExposure" />

        <Switch
            android:id="@+id/swSub"
            android:layout_width="201dp"
            android:layout_height="29dp"
            android:layout_marginTop="8dp"
            android:text="Sub Exposure"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.134"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swFace" />

        <Switch
            android:id="@+id/swP"
            android:layout_width="201dp"
            android:layout_height="30dp"
            android:layout_marginTop="12dp"
            android:text="Precautions"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.121"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swSub" />

        <EditText
            android:id="@+id/etSick"
            android:layout_width="368dp"
            android:layout_height="46dp"
            android:layout_marginTop="16dp"
            android:ems="10"
            android:hint="Since when you are feeling sick"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.339"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swP" />

        <ImageView
            android:id="@+id/ivCal"
            android:layout_width="36dp"
            android:layout_height="30dp"
            android:layout_marginStart="316dp"
            app:layout_constraintStart_toStartOf="@+id/etSick"
            app:layout_constraintTop_toTopOf="@+id/etSick"
            app:srcCompat="@drawable/cld" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Symptoms"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etSick" />

        <EditText
            android:id="@+id/etSymptoms"
            android:layout_width="202dp"
            android:layout_height="40dp"
            android:layout_marginTop="14dp"
            android:ems="10"
            android:hint="Enter Symptoms"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <ImageView
            android:id="@+id/ivAddSymptoms"
            android:layout_width="77dp"
            android:layout_height="39dp"
            android:layout_marginStart="300dp"
            android:layout_marginTop="-35dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.777"
            app:layout_constraintStart_toEndOf="@+id/etSymptoms"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            app:srcCompat="@drawable/r10" />

        <ImageView
            android:id="@+id/bAddSym"
            android:layout_width="49dp"
            android:layout_height="18dp"
            android:layout_marginStart="306dp"
            android:layout_marginTop="-32dp"
            android:layout_marginBottom="16dp"
            android:clickable="true"
            app:layout_constraintBottom_toBottomOf="@+id/ivAddSymptoms"
            app:layout_constraintEnd_toEndOf="@+id/ivAddSymptoms"
            app:layout_constraintHorizontal_bias="0.142"
            app:layout_constraintStart_toStartOf="@+id/ivAddSymptoms"
            app:srcCompat="@drawable/a" />

        <ListView
            android:id="@+id/lvSymp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            app:layout_constraintBottom_toTopOf="@+id/btnNext2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etSymptoms" />

        <Button
            android:id="@+id/btnNext2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="56dp"
            android:background="#08595D"
            android:text="NEXT"
            android:textColor="#FFFFFF"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </LinearLayout>
</ScrollView>